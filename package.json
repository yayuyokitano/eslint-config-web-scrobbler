{
  "name": "eslint-config-web-scrobbler",
  "version": "2.1.1",
  "author": "Alexey <alex.esprit@gmail.com>",
  "description": "ESLint configuration file for Web Scrobbler projects",
  "license": "MIT",
  "main": "index.js",
  "files": [
    "index.js",
    "typescript.js",
    "vue.js",
    "config/**"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/web-scrobbler/eslint-config-web-scrobbler.git"
  },
  "keywords": [
    "config",
    "eslint",
    "eslint-config"
  ],
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "eslint-plugin-jsdoc": "33.0.0"
  },
  "devDependencies": {
    "eslint": "7.23.0",
    "prettier": "^2.2.1",
    "prettier-config-web-scrobbler": "0.1.0",
    "tape": "^5.2.2"
  },
  "peerDependencies": {
    "@typescript-eslint/eslint-plugin": ">=3",
    "@typescript-eslint/parser": ">=3",
    "eslint": ">=7",
    "eslint-plugin-vue": ">=6"
  },
  "scripts": {
    "lint": "eslint --config index.js \"**/*.js\"",
    "test": "tape test/*",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "prettier": "prettier-config-web-scrobbler"
}
